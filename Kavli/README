set ylabel "Repeated n-grams"
set term pdf monochrome
set output "clusters_all_grams.pdf"
plot "/tmp/cellibrium/clusters_6_grams"  w line, "/tmp/cellibrium/clusters_3_grams"  w line, "/tmp/cellibrium/clusters_6_grams"  w line

